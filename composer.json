{
    "name": "protonduck/backend",
    "description": "API for open source bookmark manager ProtonDuck.",
    "keywords": ["bookmarking-application","bookmark-manager","selfhosted","bookmarks","bookmark","api","yii2"],
    "homepage": "https://github.com/protonduck",
    "type": "project",
    "license": "AGPL-3.0-only",
    "support": {
        "issues": "https://github.com/protonduck/bookmarks/issues",
        "source": "https://github.com/protonduck/bookmarks"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.3",
        "ext-json": "*",
        "yiisoft/yii2": "~2.0.43",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "demi/sort": "~1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-rest": "dev-master"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
